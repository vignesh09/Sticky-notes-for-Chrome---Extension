{
  "name": "Note",
  "version": "1.0",
  "manifest_version": 2,
   
  "description": "My first Chrome extension.",
 
  "browser_action": {
    "default_icon": "icons.png",
    "default_popup": "search.html"
  },
  "background": {
    "page": "popup.html"
  },
   "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["search.js"]
  }
]
}